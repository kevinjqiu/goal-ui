<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">$Team Name</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>
<div class="row">
  <div class="col-lg-6">
    <div class="panel panel-default standing-table">
      <div class="panel-heading text-center">
        Standing
      </div>
      <div class="panel-body">
        <table class="table table-bordered table-striped">
          <thead>
            <tr>
              <th>Game Day</th>
              <th>Against</th>
              <th>Score</th>
              <th>Home/Away</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>1</td>
              <td>
              {{ team-widget team_logo_url="/assets/logos/normal/ABN.png" team_name="Bournmouth" team_id="ABN" }}
              </td>
              <td>
              2:0
              </td>
              <td>
              H
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
